{
  "name": "axios/aliyun-sdk-core",
  "description": "SDK for aliyun service development",
  "keywords": [
    "aliyun",
    "aliyun-service",
    "aliyun-sdk"
  ],
  "homepage": "https://github.com/AxiosCros/aliyun-sdk-core",
  "license": "MIT",
  "authors": [
    {
      "name": "Axios",
      "email": "axioscros@aliyun.com"
    }
  ],
  "support": {
    "issues": "https://github.com/AxiosCros/aliyun-sdk-core/issues"
  },
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^5.0|^6.0|^7.2|^8.0"
  },
  "require-dev": {
    "symfony/var-dumper": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "aliyun\\sdk\\": "library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "aliyun\\sdk\\tests\\": "tests/"
    }
  },
  "scripts": {
    "fixer": [
      "php-cs-fixer fix ./"
    ],
    "test": [
      "phpunit --colors=always"
    ]
  }
}
